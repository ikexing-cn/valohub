{
  "name": "valorant-bot",
  "version": "0.0.1",
  "private": "true",
  "packageManager": "pnpm@8.15.1",
  "type": "module",
  "license": "MIT",
  "author": "ikx <ikexing.cn@gmail.com>",
  "scripts": {
    "dev": "dotenv -- turbo dev",
    "build": "dotenv -- turbo build",
    "lint": "eslint . --cache && prettier --cache -c .",
    "lint:fix": "eslint . --cache --fix && prettier --cache -w .",
    "db:generate": "dotenv -- turbo db:generate",
    "db:view": "dotenv -- turbo db:view",
    "db:push": "dotenv -- turbo db:push",
    "deploy:bot": "turbo prune @valorant-bot/bot --out-dir .deploy-bot && cp tsconfig.json .deploy-bot/tsconfig.json"
  },
  "dependencies": {
    "dotenv": "^16.4.1"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^3.7.6",
    "@sxzz/prettier-config": "^2.0.0",
    "@tqman/valoffi-api-client": "^1.1.0",
    "@types/node": "^20.11.13",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "prettier": "^3.2.4",
    "tsup": "^8.0.1",
    "turbo": "^1.12.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": "20.11.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "nitro-cors@0.7.0": "patches/nitro-cors@0.7.0.patch"
    }
  },
  "prettier": "@sxzz/prettier-config"
}
